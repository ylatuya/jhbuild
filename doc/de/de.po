"#!/bin/sh\n"
"\n"
"GNOME=<replaceable>/opt/gnome</replaceable>\n"
" \n"
"GDK_USE_XFT=1\n"
"XDG_DATA_DIRS=$XDG_DATA_DIRS:$GNOME/share\n"
"XDG_CONFIG_DIRS=$XDG_CONFIG_DIRS:$GNOME/etc/xdg\n"
"\n"
"jhbuild run gnome-session\n"
