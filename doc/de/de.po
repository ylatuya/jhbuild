"  &lt;/dependencies&gt;\n"
"  &lt;after&gt;\n"
"    &lt;dep package=\"<replaceable>modulename</replaceable>\"/&gt;\n"
"    ...\n"
"  &lt;/after&gt;\n"
"\n"
"  &lt;testedmodules&gt;\n"
"    &lt;tested package=\"<replaceable>package</replaceable>\" /&gt;\n"
"  &lt;/testedmodules&gt;\n"
"\n"
"&lt;/testmodule&gt;\n"
